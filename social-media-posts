


let name = "Admin";
let vowels = ["a", "e", "i", "o", "u"];

if (vowels.includes(name[0])) {
    console.log("Your name starts with a vowel!");
} else {
    console.log("Your name does not start with a vowel.");
}


 //A Simple Way to Check for Values


let languages = ["JavaScript", "Python", "C++", "Java"];
console.log(languages.includes("JavaScript")); // true
console.log(languages.includes("Ruby")); // false
